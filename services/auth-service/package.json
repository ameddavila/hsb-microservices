{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "start:prod": "node -r tsconfig-paths/register dist/index.js",
